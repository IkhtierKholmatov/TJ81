          RUS: |
            <p><span style="font-size:16px;color:#0000ff;font-family:'Arial CYR';">ПЕРЕПИШИТЕ ДАТУ КАЖДОЙ ВАКЦИНАЦИИ ИЗ ЗАПИСИ В ФОРМАХ 112 И 63 &#160;</span></p>
